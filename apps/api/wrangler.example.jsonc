{
  "$schema": "node_modules/wrangler/config-schema.json",
  "name": "yoncom-order",
  "main": "src/index.ts",
  "compatibility_date": "2025-04-18",
  "observability": {
    "enabled": true
  },
  "d1_databases": [
    {
      "binding": "DB",
      "database_name": "yoncom-order-db",
      "database_id": "<CLOUDFLARE_DATABASE_ID>",
      "migrations_dir": "../../packages/db/.migrations"
    }
  ]
}